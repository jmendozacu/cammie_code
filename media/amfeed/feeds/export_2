&lt;script type=&quot;text/javascript&quot;&gt;
var $i = jQuery.noConflict();
function addBodyClass()
{
if($i(window).width() &lt;= 999)
{
if(!$i(&quot;.mobile .mobile_slider ul li&quot;).hasClass('active'))
{
$i('body').addClass('mobile');
$i(&quot;.mobile .mobile_slider ul li&quot;).removeClass(&quot;active&quot;);
$i(&quot;.mobile .mobile_slider ul li&quot;).removeClass(&quot;next&quot;);
$i(&quot;.mobile .mobile_slider ul li&quot;).removeClass(&quot;prev&quot;);
$i(&quot;.mobile .mobile_slider ul li:first-child&quot;).addClass(&quot;active&quot;);
$i(&quot;.mobile .mobile_slider ul li.active&quot;).next('li').addClass(&quot;next&quot;);
$i(&quot;.mobile .mobile_slider ul li.active&quot;).prev('li').addClass(&quot;prev&quot;);
if($i(&quot;.mobile_slider li.active&quot;).next(&quot;li&quot;).hasClass(&quot;next&quot;))
{
if($i(&quot;.mobile .mobile_slider li.active&quot;).next('li').children('img').attr('src') == &quot;&quot;)
{
$i(&quot;.next_btn&quot;).css(&quot;display&quot;,&quot;none&quot;);
}
else
{
$i(&quot;.next_btn&quot;).css(&quot;display&quot;,&quot;block&quot;);
}
}
else
{
$i(&quot;.next_btn&quot;).css(&quot;display&quot;,&quot;none&quot;);
}
//prev arrow
if($i(&quot;.mobile_slider li.active&quot;).prev(&quot;li&quot;).hasClass(&quot;prev&quot;))
{
$i(&quot;.prev_btn&quot;).css(&quot;display&quot;,&quot;block&quot;);
}
else
{
$i(&quot;.prev_btn&quot;).css(&quot;display&quot;,&quot;none&quot;);
}
}
}
else
{
$i(&quot;.mobile .mobile_slider li&quot;).removeClass(&quot;active&quot;);
$i(&quot;.mobile .mobile_slider li&quot;).removeClass(&quot;next&quot;);
$i(&quot;.mobile .mobile_slider li&quot;).removeClass(&quot;prev&quot;);
$i('body').removeClass('mobile');
}
}
&lt;/script&gt; 
&lt;script type=&quot;text/javascript&quot;&gt;
window.onload = function(){addBodyClass()};
$i( window ).resize(function() {addBodyClass()});
$i(&quot;.prev_btn&quot;).on('click',function(e)
{
e.stopImmediatePropagation();
$i(&quot;.mobile .mobile_slider li img&quot;).trigger(&quot;click&quot;);
$i(&quot;.mobile .mobile_slider li.active&quot;).removeClass(&quot;active&quot;);
$i(&quot;.mobile .mobile_slider li.prev&quot;).addClass(&quot;active&quot;);
$i(&quot;.mobile .mobile_slider li.active&quot;).removeClass('prev');
$i(&quot;.mobile .mobile_slider li.active&quot;).prev(&quot;li&quot;).addClass('prev');
$i(&quot;.mobile .mobile_slider li.next&quot;).removeClass(&quot;next&quot;);
$i(&quot;.mobile .mobile_slider li.active&quot;).next(&quot;li&quot;).addClass('next');
if(!$i(&quot;.mobile .mobile_slider li.active&quot;).prev().hasClass('thum-resize-bg'))
{
$i(&quot;.prev_btn&quot;).css(&quot;display&quot;,&quot;none&quot;);
}
if($i(&quot;.mobile .mobile_slider li.active&quot;).next().hasClass('thum-resize-bg'))
{
$i(&quot;.mobile .mobile_slider li.next&quot;).removeClass(&quot;next&quot;);
$i(&quot;.mobile .mobile_slider li.active&quot;).next(&quot;li&quot;).addClass(&quot;next&quot;);
if($i(&quot;.mobile .mobile_slider li.active&quot;).next().hasClass('thum-resize-bg'))
{
$i(&quot;.next_btn&quot;).css(&quot;display&quot;,&quot;block&quot;);
}
else
{
$i(&quot;.next_btn&quot;).css(&quot;display&quot;,&quot;none&quot;);
}
}
});
$i(&quot;.next_btn&quot;).on('click',function(e)
{
e.stopImmediatePropagation();
if($i(&quot;.mobile .mobile_slider li.active&quot;).next().hasClass('thum-resize-bg'))
{
$i(&quot;.mobile .mobile_slider li img&quot;).trigger(&quot;click&quot;);
$i(&quot;.mobile .mobile_slider li.active&quot;).removeClass(&quot;active&quot;);
$i(&quot;.mobile .mobile_slider li.next&quot;).addClass(&quot;active&quot;);
$i(&quot;.mobile .mobile_slider li.next&quot;).removeClass(&quot;next&quot;);
$i(&quot;.mobile .mobile_slider li.active&quot;).next('li').addClass(&quot;next&quot;);
if($i(&quot;.mobile .mobile_slider li.active&quot;).prev().hasClass('thum-resize-bg'))
{
$i(&quot;.mobile .mobile_slider li.prev&quot;).removeClass(&quot;prev&quot;);
$i(&quot;.mobile .mobile_slider li.active&quot;).prev(&quot;li&quot;).addClass(&quot;prev&quot;);
if($i(&quot;.mobile .mobile_slider li.active&quot;).prev().hasClass('thum-resize-bg'))
{
$i(&quot;.prev_btn&quot;).css(&quot;display&quot;,&quot;block&quot;);
}
else
{
$i(&quot;.prev_btn&quot;).css(&quot;display&quot;,&quot;none&quot;);
}
}
if(!$i(&quot;.mobile .mobile_slider li.active&quot;).next().hasClass('thum-resize-bg'))
{
$i(&quot;.next_btn&quot;).css(&quot;display&quot;,&quot;none&quot;);
}
if($i(&quot;.mobile .mobile_slider li.active&quot;).next('li').children('img').attr('src') == &quot;&quot;)
{
$i(&quot;.next_btn&quot;).css(&quot;display&quot;,&quot;none&quot;);
}
}
});
$i(document).ready(function(){
$i(&quot;#nav &gt; li &gt; a.open&quot;).css('display','block');
$i(&quot;#nav &gt; li &gt; a&quot;).click(function(e){
if($i(this).attr(&quot;href&quot;) == &quot;javascript:void(0)&quot;)
{
if($i(this).parent().has(&quot;ul&quot;)) {
e.preventDefault();
}
if(!$i(this).hasClass(&quot;open&quot;)) {
// hide any open menus and remove all other classes
$i(&quot;#nav li ul&quot;).slideUp(&quot;slow&quot;);
$i(&quot;#nav li a&quot;).removeClass(&quot;open&quot;);
// open our new menu and add the open class
$i(this).next(&quot;ul&quot;).slideToggle(&quot;slow&quot;);
$i(this).addClass(&quot;open&quot;);
}
else if($i(this).hasClass(&quot;open&quot;)) {
$i(this).removeClass(&quot;open&quot;);
$i(this).next(&quot;ul&quot;).slideUp(&quot;slow&quot;);
}
}

});
});
&lt;/script&gt; 

&lt;script type=&quot;text/javascript&quot;&gt;
function ssmakebtns(){
var itm, d=document, c=&quot;000000000000&quot;,
url=location.href, site=location.hostname
.split(/.ebay.|.ebaydesc./i)[1],
trk=d.getElementById(&quot;sstrackthis&quot;),
asq=d.getElementById(&quot;ssasqthis&quot;),
crt=d.getElementById(&quot;sscartthis&quot;),
bin=d.getElementById(&quot;ssbinthis&quot;),
a=url.match(/item=d+/);
if(!a){a=url.match(//d+/);
if(!a){a=url.match(/d{12}/)}}
if(a){itm=a[0].match(/d+/)}
ssrevbtns=function(p){
for(j in p){if(p[j]&amp;&amp;itm)
{p[j].href=p[j].href.split(c).join(itm)}
else if(p[j]){p[j].style.display=&quot;none&quot;}}}
ssrevbtns([trk,asq,crt,bin]);}
ssmakebtns();&lt;/script&gt;
&lt;script type=&quot;text/javascript&quot;&gt;
function sschangesite(){try{
var d=document, loc=location.hostname
.split(/.ebay.|.ebaydesc./i)[1]
.split(///)[0], k=d.links;
for(var j=0;j&lt;k.length;j++){
if(k[j].className==&quot;btn267&quot;){
var old=k[j].href.split(/.ebay./i)[1]
.split(///)[0];k[j].href=k[j].href
.split(old).join(loc)}}}catch(e){};}
sschangesite();&lt;/script&gt;

&lt;!-- end of eBay QuickyButtons --&gt;


&lt;!-- Scripted buttons are not allowed on blogs or forums. --&gt;
&lt;!-- Ask and Watch will only function in an active Listing --&gt;
